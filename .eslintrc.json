{
  "env": {
    "browser": true
  },
  "globals": {
    "requie": true
  },
  "extends": [
    "standard",
    "standard-react",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "jsx-quotes": [2, "prefer-double"],
    "react/jsx-no-bind": 0
  }
}
